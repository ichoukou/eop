<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
<validators>
	<field name="channel.key">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>属性名不能为空</message>
		</field-validator>
	</field>
	<field name="channel.value">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>属性值不能为空</message>
		</field-validator>
	</field>
	<field name="channel.ipAddress">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>IP地址不能为空</message>
		</field-validator>
	</field>
	<field name="channel.clientId">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>ClientId不能为空</message>
		</field-validator>
	</field>
</validators>