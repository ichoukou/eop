<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
<validators>
	<field name="configCode.confKey">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>请输入配置名</message>
		</field-validator>
	</field>
	<field name="configCode.confValue">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>请输入配置值</message>
		</field-validator>
	</field>
	<field name="configCode.confText">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>请输入配置显示值</message>
		</field-validator>
	</field>
	<field name="configCode.seq">
		<field-validator type="required">
			<message>请输入排序号</message>
		</field-validator>
	</field>
</validators>